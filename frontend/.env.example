# Backend API URL
VITE_API_BASE_URL=http://localhost:5000/api

# Set to 'true' only if Google OAuth is configured in backend
VITE_ENABLE_GOOGLE_OAUTH=false

# Google Maps API Key (optional - for embedded maps in User Sessions)
# Get your key from: https://console.cloud.google.com/google/maps-apis
# If not set, maps will open in a new tab instead of embedding
VITE_GOOGLE_MAPS_API_KEY=your_api_key_here
